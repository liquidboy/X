<UserControl
    x:Class="X.ModernDesktop.SimTower.Views.StartMenu"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="300"
    d:DesignWidth="400">

  <StackPanel Orientation="Vertical" HorizontalAlignment="Stretch">
    <Button x:Name="butNewTower" Content="New Tower" HorizontalAlignment="Stretch" Click="butNewTower_Click" ></Button>
    <Button x:Name="butLoadTower" Content="Load Tower" HorizontalAlignment="Stretch" Margin="0,10,0,0" Click="butLoadTower_Click"></Button>
  </StackPanel>
</UserControl>
